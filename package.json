{
  "name": "favand-eleventy-jsx",
  "version": "1.0.0",
  "description": "Eleventy website for favand.net",
  "main": "index.js",
  "scripts": {
    "dev": "ELEVENTY_EXPERIMENTAL=true eleventy --serve",
    "build": "ELEVENTY_EXPERIMENTAL=true eleventy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .jsx,.js",
    "lint:fix": "eslint . --ext .jsx,.js --fix",
    "lint:githook": "eslint --ext .jsx,.js --fix --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:githook"
    ]
  },
  "author": "Daniel Favand",
  "license": "GPL-3.0",
  "dependencies": {
    "@11ty/eleventy": "0.11.0",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.0",
    "@types/react": "^16.9.53",
    "@types/react-helmet": "^6.1.0",
    "babel-loader": "^8.1.0",
    "eleventy-plugin-react": "../eleventy-plugin-react",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "typescript": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "purgecss": "^3.0.0"
  }
}